<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Classes">
  <resultMap id="classes" type="com.syl.model.Classes" >
		<id column="id" property="id" jdbcType="VARCHAR" />               
		<result column="className" property="className" jdbcType="VARCHAR" />
		<result column="classLevel" property="classLevel" jdbcType="VARCHAR" />   
		<result column="classCount" property="classCount" jdbcType="VARCHAR" />   
  </resultMap>
  
  <select id="selectAllClasses" resultMap="classes">
  	select * from classes
  </select>
  
  
  
</mapper>